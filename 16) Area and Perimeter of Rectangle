#include <stdio.h>

int main() {
    float s, w, a, p;

    printf("Enter length of the rectangle: ");
    scanf("%f", &s);

    printf("Enter width of the rectangle: ");
    scanf("%f", &w);

    a = s * w;
    p = 2 * (s + w);

    printf("Area of rectangle = %.2f\n", a);
    printf("Perimeter of rectangle = %.2f\n", p);

    return 0;
}
