// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int arr[5][5],i,j;
    printf("ENTER 5 NUMBERS - \n");
    for(i=0;i<5;i++){
        for(j=0;j<5;j++);
        scanf("%d",&arr[i][j]);
    }
    printf("the entered numbers are ; \n");
    for(i=0;i<5;i++){
        for(j=0;j<5;j++);
        printf("%d %d\n",arr[i][j]);
    }
    return 0;
    }
